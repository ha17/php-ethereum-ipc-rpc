{
    "name": "ha17/php-ethereum-ipc-rpc",
    "description": "PHP SDK for interacting with the Ethereum API via IPC or RPC",
    "type": "library",
    "require": {
        "jsonrpc/jsonrpc": "^1.0",
        "litipk/php-bignumbers": "^0.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.5 || ^5.0.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Hans Anderson",
            "email": "me@ha17.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Ethereum\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
